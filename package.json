{
  "name": "gatsby-starter-typescript",
  "version": "1.0.0",
  "description": "Gatsby TypeScript starter",
  "keywords": [
    "gatsby, typescript"
  ],
  "license": "MIT",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "contributors": [
    "fabien0102 <fabien0102@gmail.com>",
    "Hays Clark <hays.clark@gmail.com>"
  ],
  "main": "n/a",
  "scripts": {
    "analyze": "cross-env ANALYZE=true gatsby build",
    "build": "gatsby build",
    "build-storybook": "build-storybook",
    "dev": "gatsby develop",
    "fix": "run-p fix:*",
    "fix:json": "prettier --write '**/*.json'",
    "fix:script": "eslint --fix '**/*.{js,ts,tsx}'",
    "fix:style": "stylelint --fix '**/*.scss'",
    "lint": "run-p lint:*",
    "lint-staged": "lint-staged",
    "lint:script": "eslint '**/*.{js,ts,tsx}'",
    "lint:style": "stylelint '**/*.scss'",
    "serve": "browser-sync start --server 'public'",
    "storybook": "start-storybook -p 6006",
    "test": "echo \"Error: no test specifid\" && exit 1",
    "typecheck": "tsc --noEmit -p tsconfig.production.json",
    "typo": "cspell 'src/**/*.{scss,ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-p typecheck lint-staged"
    }
  },
  "browserslist": [
    "> 0.5% in jp",
    "node >= 10",
    "not ios < 10.3",
    "not android < 5",
    "not safari < 10.1",
    "not chrome 49",
    "not dead"
  ],
  "dependencies": {
    "date-fns": "^2.11.0",
    "gatsby": "^2.19.43",
    "gatsby-image": "^2.2.43",
    "gatsby-link": "^2.2.30",
    "gatsby-plugin-react-helmet": "^3.1.23",
    "gatsby-plugin-typescript": "^2.2.3",
    "gatsby-react-router-scroll": "^2.1.23",
    "gatsby-remark-prismjs": "^3.4.1",
    "gatsby-transformer-remark": "^2.7.1",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.19.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@babel/node": "^7.8.7",
    "@babel/register": "^7.8.6",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-backgrounds": "^5.3.17",
    "@storybook/addon-info": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-viewport": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@types/node": "^13.9.1",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "contentful": "^7.14.0",
    "cross-env": "^7.0.2",
    "cspell": "^4.0.55",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "eslint-plugin-react-redux": "^3.0.3",
    "eslint-plugin-unicorn": "^17.2.0",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-google-analytics": "^2.1.37",
    "gatsby-plugin-graphql-codegen": "^2.5.0",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.1.30",
    "gatsby-plugin-sharp": "^2.4.12",
    "gatsby-plugin-sitemap": "^2.2.29",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-source-contentful": "^2.1.97",
    "gatsby-source-filesystem": "^2.1.55",
    "gatsby-transformer-sharp": "^2.3.18",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "node-sass": "^4.13.1",
    "node-sass-package-importer": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "postcss-calc": "^7.0.2",
    "postcss-color-hex-alpha": "^5.0.3",
    "postcss-custom-properties": "^9.1.1",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-image-set-function": "^3.0.1",
    "postcss-initial": "^3.0.2",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "^3.7.1",
    "sass-loader": "^8.0.2",
    "sort-package-json": "^1.40.0",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.14.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.8.3"
  }
}
