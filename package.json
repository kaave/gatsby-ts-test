{
  "name": "gatsby-starter-typescript",
  "version": "1.0.0",
  "description": "Gatsby TypeScript starter",
  "keywords": [
    "gatsby, typescript"
  ],
  "license": "MIT",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "main": "n/a",
  "scripts": {
    "analyze": "cross-env ANALYZE=true gatsby build",
    "build": "gatsby build",
    "build-storybook": "build-storybook",
    "dev": "gatsby develop",
    "fix": "run-p fix:*",
    "fix:json": "prettier --write '**/*.json'",
    "fix:script": "eslint --fix '**/*.{js,ts,tsx}'",
    "fix:style": "stylelint --fix '**/*.scss'",
    "lint": "run-p lint:*",
    "lint-staged": "lint-staged",
    "lint:script": "eslint '**/*.{js,ts,tsx}'",
    "lint:style": "stylelint '**/*.scss'",
    "serve": "browser-sync start --server 'public'",
    "storybook": "start-storybook -p 6006",
    "test": "echo \"Error: no test specifid\" && exit 1",
    "typecheck": "tsc --noEmit -p tsconfig.production.json",
    "typo": "cspell 'src/**/*.{scss,ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-p typecheck lint-staged"
    }
  },
  "browserslist": [
    "> 0.5% in jp",
    "node >= 10",
    "not ios < 10.3",
    "not android < 5",
    "not safari < 10.1",
    "not chrome 49",
    "not dead"
  ],
  "dependencies": {
    "date-fns": "^2.8.1",
    "gatsby": "^2.18.11",
    "gatsby-image": "^2.2.36",
    "gatsby-link": "^2.2.27",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-typescript": "^2.1.23",
    "normalize.css": "^8.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@babel/node": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-backgrounds": "^5.2.8",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@types/node": "^12.12.17",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "@types/react-helmet": "^5.0.14",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "contentful": "^7.11.0",
    "cross-env": "^6.0.3",
    "cspell": "^4.0.37",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-react-redux": "^3.0.2",
    "eslint-plugin-unicorn": "^14.0.1",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-google-analytics": "^2.1.31",
    "gatsby-plugin-graphql-codegen": "^2.0.1",
    "gatsby-plugin-sass": "^2.1.26",
    "gatsby-plugin-sharp": "^2.3.7",
    "gatsby-plugin-sitemap": "^2.2.24",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-source-contentful": "^2.1.69",
    "gatsby-source-filesystem": "^2.1.42",
    "gatsby-transformer-sharp": "^2.3.9",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "node-sass-package-importer": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "postcss-calc": "^7.0.1",
    "postcss-color-hex-alpha": "^5.0.3",
    "postcss-custom-properties": "^9.0.2",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-image-set-function": "^3.0.1",
    "postcss-initial": "^3.0.2",
    "postcss-url": "^8.0.0",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "sort-package-json": "^1.31.0",
    "stylelint": "^12.0.0",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-config-recommended-scss": "^4.1.0",
    "stylelint-scss": "^3.13.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.3"
  },
  "contributors": [
    "fabien0102 <fabien0102@gmail.com>",
    "Hays Clark <hays.clark@gmail.com>"
  ]
}
